{
  "name": "myanili",
  "version": "2.7.0",
  "description": "MyAniLi",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "docker pull webdevops/php-apache-dev:8.0 && docker-compose build --no-cache",
    "dev:build": "npm run build && npm run dev",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "dev": "npm run start && docker exec -ti -w /frontend -u application myanili_app npm run dev",
    "shell": "npm run start && docker exec -ti -w /frontend -u application myanili_app bash"
  },
  "author": "Christopher Schreiner",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@elunic/ecs": "^0.3.3",
    "commitlint": "^11.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
