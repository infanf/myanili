{
  "name": "myanili-frontend",
  "version": "2.29.2",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host=0.0.0.0",
    "start": "ng serve",
    "build": "npm run build:production",
    "build:production": "npm ci && ng build -c deployment,production",
    "build:beta": "npm ci && ng build -c deployment,beta",
    "test": "ISTANBUL_ADD_REPORTERS=text-summary ng test",
    "test:ci": "ISTANBUL_ADD_REPORTERS=text,json-summary KARMA_ADD_REPORTERS=junit ng test --watch=false --progress=false && npm run check",
    "check": "ng lint --type-check --tslint-config tslint.fix.json --format codeFrame && prettier --config ../.prettierrc --check 'src/**/*.{ts,js,html,css,scss,sass,less}'",
    "fix": "ng lint --type-check --fix --tslint-config tslint.fix.json --format codeFrame && prettier --config ../.prettierrc --write 'src/**/*.{ts,js,html,css,scss,sass,less}'",
    "e2e": "ng e2e"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.6",
    "@angular/common": "^16.2.6",
    "@angular/compiler": "^16.2.6",
    "@angular/core": "^16.2.6",
    "@angular/forms": "^16.2.6",
    "@angular/localize": "^16.2.6",
    "@angular/platform-browser": "^16.2.6",
    "@angular/platform-browser-dynamic": "^16.2.6",
    "@angular/pwa": "^16.2.3",
    "@angular/router": "^16.2.6",
    "@angular/service-worker": "^16.2.6",
    "@ng-bootstrap/ng-bootstrap": "^15.1.0",
    "@popperjs/core": "^2.11.7",
    "@urql/core": "^4.1.3",
    "@zma-lab/user-geolocation": "^1.1.32",
    "angular-svg-icon": "^16.1.0",
    "any-date-parser": "^1.5.3",
    "bootstrap": "~5.3.0",
    "bootstrap-icons": "^1.11.1",
    "country-code-emoji": "^2.3.0",
    "crypto-js": "^4.1.1",
    "graphql": "^16.8.1",
    "js-base64": "^3.7.3",
    "luxon": "^3.4.0",
    "ng-circle-progress": "^1.7.1",
    "ng-in-viewport": "^16.0.0",
    "ngx-device-detector": "^6.0.0",
    "rxjs": "~7.8.1",
    "stream-browserify": "^3.0.0",
    "string-similarity": "^4.0.4",
    "timezone-enum": "^1.0.1",
    "tslib": "^2.1.0",
    "xml-js": "^1.6.11",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular/cli": "^16.2.3",
    "@angular/compiler-cli": "^16.2.6",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "^3.6.3",
    "@types/luxon": "^3.2.0",
    "@types/node": "^20.5.2",
    "@types/string-similarity": "^4.0.0",
    "@types/zen-observable": "^0.8.3",
    "codelyzer": "^0.0.28",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^3.0.0",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~5.1.6"
  }
}
