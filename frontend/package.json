{
  "name": "myanili-frontend",
  "version": "2.10.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host=0.0.0.0",
    "start": "ng serve",
    "build": "npm i && npm run build:production",
    "build:production": "ng build -c production",
    "build:staging": "ng build -c staging",
    "test": "ISTANBUL_ADD_REPORTERS=text-summary ng test",
    "test:ci": "ISTANBUL_ADD_REPORTERS=text,json-summary KARMA_ADD_REPORTERS=junit ng test --watch=false --progress=false && npm run check",
    "check": "ng lint --type-check --tslint-config tslint.fix.json --format codeFrame && prettier --config ../.prettierrc --check 'src/**/*.{ts,js,html,css,scss,sass,less}'",
    "fix": "ng lint --type-check --fix --tslint-config tslint.fix.json --format codeFrame && prettier --config ../.prettierrc --write 'src/**/*.{ts,js,html,css,scss,sass,less}'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.1",
    "@angular/common": "^13.2.1",
    "@angular/compiler": "^13.2.1",
    "@angular/core": "^13.2.1",
    "@angular/forms": "^13.2.1",
    "@angular/localize": "^13.2.1",
    "@angular/platform-browser": "^13.2.1",
    "@angular/platform-browser-dynamic": "^13.2.1",
    "@angular/pwa": "^13.2.1",
    "@angular/router": "^13.2.1",
    "@angular/service-worker": "^13.2.1",
    "@apollo/client": "^3.3.11",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0-beta.4",
    "@popperjs/core": "^2.10.2",
    "angular-svg-icon": "^13.0.0",
    "apollo-angular": "^2.6.0",
    "bootstrap": "^5.0.0",
    "bootstrap-icons": "^1.8.0",
    "country-code-emoji": "^2.3.0",
    "crypto-js": "^4.1.1",
    "graphql": "^15.5.0",
    "js-base64": "^3.6.0",
    "moment": "^2.29.1",
    "ng-circle-progress": "^1.6.0",
    "ngx-device-detector": "^3.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.2",
    "@angular/cli": "^13.2.2",
    "@angular/compiler-cli": "^13.2.1",
    "@types/crypto-js": "^4.0.2",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^12.20.0",
    "@types/zen-observable": "^0.8.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4"
  }
}