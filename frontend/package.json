{
  "name": "myanili-frontend",
  "version": "2.15.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host=0.0.0.0",
    "start": "ng serve",
    "build": "npm ci && npm run build:production",
    "build:production": "ng build -c production",
    "build:staging": "ng build -c staging",
    "test": "ISTANBUL_ADD_REPORTERS=text-summary ng test",
    "test:ci": "ISTANBUL_ADD_REPORTERS=text,json-summary KARMA_ADD_REPORTERS=junit ng test --watch=false --progress=false && npm run check",
    "check": "ng lint --type-check --tslint-config tslint.fix.json --format codeFrame && prettier --config ../.prettierrc --check 'src/**/*.{ts,js,html,css,scss,sass,less}'",
    "fix": "ng lint --type-check --fix --tslint-config tslint.fix.json --format codeFrame && prettier --config ../.prettierrc --write 'src/**/*.{ts,js,html,css,scss,sass,less}'",
    "e2e": "ng e2e"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.3",
    "@angular/common": "^14.2.3",
    "@angular/compiler": "^14.2.3",
    "@angular/core": "^14.2.3",
    "@angular/forms": "^14.2.3",
    "@angular/localize": "^14.2.3",
    "@angular/platform-browser": "^14.2.3",
    "@angular/platform-browser-dynamic": "^14.2.3",
    "@angular/pwa": "^14.2.3",
    "@angular/router": "^14.2.3",
    "@angular/service-worker": "^14.2.3",
    "@apollo/client": "~3.6.0",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@popperjs/core": "^2.10.2",
    "angular-svg-icon": "^14.0.0",
    "any-date-parser": "^1.5.3",
    "apollo-angular": "^4.0.1",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "country-code-emoji": "^2.3.0",
    "crypto-js": "^4.1.1",
    "graphql": "^16.6.0",
    "js-base64": "^3.6.0",
    "luxon": "^3.0.1",
    "ng-circle-progress": "^1.6.0",
    "ng-in-viewport": "^13.0.1",
    "ngx-device-detector": "^4.0.0",
    "rxjs": "~6.6.0",
    "string-similarity": "^4.0.4",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.2.3",
    "@types/crypto-js": "^4.0.2",
    "@types/jasmine": "^3.6.3",
    "@types/luxon": "^3.0.0",
    "@types/node": "^16.11.49",
    "@types/string-similarity": "^4.0.0",
    "@types/zen-observable": "^0.8.3",
    "codelyzer": "^0.0.28",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.2"
  }
}
